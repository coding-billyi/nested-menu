    "@types/react-dom": "^17.0.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
